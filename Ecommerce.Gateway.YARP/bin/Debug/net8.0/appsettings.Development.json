{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "productsRoutes": {
        "ClusterId": "productsCluster",
        "Match": {
          "Path": "/api/Product/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "productsCluster": {
        "Destinations": {
          "productsDestinations": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    }
  }
}
